{
	"useTabs": false,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"printWidth": 100,
	"singleAttributePerLine": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}
